<div class="container justify-content-center text-center">
  <ul class="list-inline ">
    <% categories.each do |category| %>
      <li class="list-inline-item">
        <%= link_to results_path(category: category.id) do %>
          <%= image_tag("categories/#{category.name.downcase}.png", width: 60) %>
          <h4><%= category.name %></h4>
        <% end %>
      </li>
    <% end %>
  </li>
</ul>
</div>

