<div class="container justify-content-center text-center">
  <ul class="list-inline ">
    <% categories.each do |category| %>
      <li class="list-inline-item">
        <%= link_to root_path do %>
          <h5><%= category.name %></h5>
          <i class="fa-solid fa-trees"></i>
        <% end %>
      </li>
    <% end %>
  </li>
</ul>
</div>
